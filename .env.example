# NAVI Environment Variables - Example Configuration
# Copy this file to .env and fill in your actual values

# Base URL (set this to your production domain)
BASE_URL=http://localhost:4999

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_from_botfather
TELEGRAM_BOT_USERNAME=your_bot_username

# Google AI Configuration
GEMINI_API_KEY=your_gemini_api_key_from_google_ai_studio

# Google OAuth Configuration  
GOOGLE_CLIENT_ID=your_google_oauth_client_id
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret

# Flask Configuration
FLASK_SECRET_KEY=your_flask_secret_key_generate_random_string
FLASK_DEBUG=False

# Server Configuration
PORT=4999

# Optional: Google Service Account (for enhanced features)
GOOGLE_APPLICATION_CREDENTIALS=path_to_service_account.json

# Optional: Development/Testing
NAVI_TEST_MODE=false
NAVI_TEST_USER_EMAIL=test@example.com